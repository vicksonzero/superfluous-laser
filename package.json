{
  "name": "superfluous-laser",
  "version": "1.0.0",
  "description": "",
  "main": "server-dist/server-src/server.js",
  "scripts": {
    "start": "npm run build-client && webpack-dev-server --mode production",
    "build": "npm run build-client",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Server-ts,Server,Client\" -c \"yellow.bold,cyan.bold,magenta.bold\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-webpack\"",
    "dev-l": "concurrently -k -p \"[{name}]\" -n \"Server-ts,Server,Client\" -c \"yellow.bold,cyan.bold,magenta.bold\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-webpack\" | tee concurrently.log",
    "dev-server": "concurrently -k -p \"[{name}]\" -n \"Server-ts,Server\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "dev-client": "npm run watch-webpack",
    "itch": "npm run build && npm run create-itch-zip",
    "test": "npm run lint",
    "lint": "eslint client-src --ext .js,.ts,.tsx",
    "create-itch-zip": "node scripts/create-itch-zip.js",
    "build-client": "webpack --mode production",
    "watch-webpack": "webpack-dev-server --progress --color --mode development --config webpack.config.js",
    "watch-node": "set FORCE_COLOR=2&& nodemon .",
    "watch-ts": "tsc -w",
    "webpack": "webpack",
    "tsc": "tsc",
    "client-size-analyse": "webpack --profile --json > stats.json && npx webpack-bundle-analyzer stats.json",
    "a": ""
  },
  "author": {
    "name": "Dickson Chui (@vicksonzero)",
    "email": "dicksonZero@gmail.com"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@flyover/box2d": "^1.0.1",
    "@types/seedrandom": "^2.4.28",
    "bson": "^4.2.0",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "js-base64": "^3.5.2",
    "js-yaml": "^3.13.1",
    "seedrandom": "^3.0.5",
    "socket.io": "^3.0.0",
    "socket.io-client": "^3.0.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.8",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/js-yaml": "^3.12.3",
    "@types/node": "^14.14.6",
    "@types/socket.io-client": "^1.4.34",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "archiver": "^5.0.2",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^5.1.1",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "expose-loader": "^0.7.5",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.6",
    "phaser": "^3.21.0",
    "raw-loader": "^4.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "yaml-loader": "^0.5.0"
  }
}
